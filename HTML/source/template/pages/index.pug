							.testimonials-slider__author
								img(src="img/quotes.png" alt="img")
								h6 Well Singer
								span Manager of Westwood

						.testimonials-slider__item

							p “Rockling Devario deep sea bonefish cutthroat trout streamer fish kaluga sailback scorpionfish sand dab, turkeyfish golden loach sand diver. Leopard danio píntano bonnetmouth; blue whiting, suckermouth armored catfish luderick blackchin kingfish.
							p Midshipman, lightfish longfin smelt pickerel houndshark whiptail. Barracuda archerfish slimehead broadband dogfish, Pacific hake false trevally queen parrotfish Black prickleback blenny, bigeye squaretail nurseryfish yellowtail barracuda. Halibut: Blacksmelt”

							.testimonials-slider__author
								img(src="img/quotes.png" alt="img")
								h6 Well Singer
								span Manager of Westwood

	// section end

	include ../blocks/_front-blog/_front-blog

	// section start

	section.section.pt-0.bg--lgray
		.container
			.row.bottom-70

				.col-12

					.heading.heading--center
						span.heading__pre-title Partners
						h3.heading__title Companies who #[span.color--green Trust] us
						span.heading__layout.layout--lgray Partners

			.row

				.col-12

					include ../blocks/_logos-slider/_logos-slider

	// section end

	// section start

	section.section
		img.img--bg(src="img/action-bg.jpg" alt="bg")

		.container
			.row

				.col-lg-8.col-xl-6

					.form-wrapper

						include ../blocks/_contact-form/_contact-form

	// section end