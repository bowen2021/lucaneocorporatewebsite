extends ../layouts/primary.pug

block variables

	-var page = 'index'

	-var type = 'front'

block main

	include ../blocks/_promo/_promo

	// section start

	section.section
		.container
			.row.bottom-70

				.col-lg-4

					.heading.bottom-40
						span.heading__pre-title Services
						h3.heading__title Transporterium Services
						span.heading__layout Services

					a.button.button--green.d-none.d-lg-inline-block(href="#") #[span All services] #[+svg('arrow')]

				.col-lg-8

					p #[strong Rockling Devario deep sea bonefish cutthroat trout streamer fish kaluga sailback scorpionfish sand dab, turkeyfish golden loach sand diver. Leopard danio píntano bonnetmouth; blue whiting, suckermouth armored catfish luderick blackchin kingfish. Midshipman, lightfish longfin smelt pickerel houndshark whipt.]
					p.bottom-0 Pacific hake false trevally queen parrotfish Black prickleback humuhumunukunukuapua'a mosshead warbonnet sweeper! Greenling sleeper; Owens pupfish large-eye bream kokanee sprat shrimpfish grunter, Ratfish combtooth blenny, bigeye squaretail nurseryfish yellowtail barracuda. Halibut: mosshead warbonnet sweeper! Greenling sleeper;

			.row.offset-50

				.col-sm-6.col-xl-3

					.count-item

						.count-item__img
							img(src="img/front-serv_1.png" alt="icon")

						h6.count-item__title #[span.count-item__count 01] #[span Truck Freight]

				.col-sm-6.col-xl-3

					.count-item

						.count-item__img
							img(src="img/front-serv_2.png" alt="icon")

						h6.count-item__title #[span.count-item__count 02] #[span Shipping Freight]

				.col-sm-6.col-xl-3

					.count-item

						.count-item__img
							img(src="img/front-serv_3.png" alt="icon")

						h6.count-item__title #[span.count-item__count 03] #[span Plane Freight]

				.col-sm-6.col-xl-3

					.count-item

						.count-item__img
							img(src="img/front-serv_4.png" alt="icon")

						h6.count-item__title #[span.count-item__count 04] #[span Train Freight]

			.row.top-70.d-flex.d-lg-none

				.col-12.text-center

					a.button.button--green(href="#") #[span All services] #[+svg('arrow')]

	// section end

	include ../blocks/_front-about/_front-about

	// section start

	section.section.bg--lgray
		.container
			.row.align-items-end.bottom-70

				.col-lg-5.col-xl-4

					.heading
						span.heading__pre-title Features
						h3.heading__title Powerfull Features of #[span.color--green TransX] company
						span.heading__layout.layout--lgray Features

			.row.offset-50

				.col-sm-6.col-lg-3

					.icon-item

						.icon-item__img
							+svg('warehouse')

						h6.icon-item__title Warehouse

						p.icon-item__text Kokanee sprat shrimpfish. Pacific hake false trevally queen parrotfish Black prickleback

				.col-sm-6.col-lg-3

					.icon-item

						.icon-item__img
							+svg('support')

						h6.icon-item__title Support 24/7

						p.icon-item__text Greenling sleeper; Owens pupfish large-eye bream kokanee sprat shrimpfish. Pacific hake false 

				.col-sm-6.col-lg-3

					.icon-item

						.icon-item__img
							+svg('location')

						h6.icon-item__title Online Tracking

						p.icon-item__text Mosshead warbonnet sweeper! Greenling sleeper; Owens pupfish large-eye bream kokanee sprat 

				.col-sm-6.col-lg-3

					.icon-item

						.icon-item__img
							+svg('insurance')

						h6.icon-item__title Cargo Insurance

						p.icon-item__text Pacific hake false trevally queen parrotfish Black prickleback humuhumunukunukuapua'a

	// section end

	// section start

	section.section
		.container
			.row

				.col-12

					img(src="img/map-transx.png" alt="img")

			.row.offset-50

				.col-md-6.col-lg-4.text-center

					+counter--blue('5', 'millions', 'Delivered packeges')

				.col-md-6.col-lg-4.text-center

					+counter--blue('2', 'million', 'Miles driven each of the year')

				.col-md-6.col-lg-4.text-center

					+counter--blue('50', 'years', 'Total warehouse area')

	// section end

	include ../blocks/_quote-section/_quote-section

	// section start

	section.section.bg--lgray
		.container
			.row.bottom-50

				.col-12

					.heading.heading--center
						span.heading__pre-title Pricing
						h3.heading__title The right price for you, #[span.color--green whoever] you are
						span.heading__layout.layout--lgray Pricing

			.row.offset-50

				.col-sm-9.col-md-6.col-xl-3

					.pricing-plan.pricing-plan--bordered.pricing-plan--small

						.pricing-plan__inner

							h5.pricing-plan__title One pack
							.pricing-plan__price #[span 95] #[span usd]
							ul.list.list--check.list--reset
								li.list__item 1 warehouse
								li.list__item Custom business rules
								li.list__item.item--disabled Real-time rate shopping
								li.list__item.item--disabled 100 freight shipments
							+button--green('#', 'Get started')

				.col-sm-9.col-md-6.col-xl-3

					.pricing-plan.pricing-plan--bordered.pricing-plan--small

						.pricing-plan__inner

							h5.pricing-plan__title Starter pack
							.pricing-plan__price #[span 100] #[span usd]
							ul.list.list--check.list--reset
								li.list__item 1 warehouse
								li.list__item Custom business rules
								li.list__item.item--disabled Real-time rate shopping
								li.list__item.item--disabled 100 freight shipments
							+button--green('#', 'Get started')

				.col-sm-9.col-md-6.col-xl-3

					.pricing-plan.pricing-plan--bordered.pricing-plan--small

						.pricing-plan__inner

							h5.pricing-plan__title Regular #[span.color--green pack]
							.pricing-plan__price #[span 150] #[span usd]
							ul.list.list--check.list--reset
								li.list__item 1 warehouse
								li.list__item Custom business rules
								li.list__item Real-time rate shopping
								li.list__item.item--disabled 100 freight shipments
							+button--green('#', 'Get started')

				.col-sm-9.col-md-6.col-xl-3

					.pricing-plan.pricing-plan--blue.pricing-plan--small

						img.pricing-plan__bg(src="img/price_background.png" alt="img")

						.pricing-plan__inner

							h5.pricing-plan__title Premium pack
							.pricing-plan__price #[span 255] #[span usd]
							ul.list.list--check.list--reset
								li.list__item 1 warehouse
								li.list__item Custom business rules
								li.list__item Real-time rate shopping
								li.list__item 100 freight shipments
							+button--white('#', 'Get started')

	// section end

	include ../blocks/_front-gallery/_front-gallery

