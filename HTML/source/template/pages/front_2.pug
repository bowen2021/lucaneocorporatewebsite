										+svg('pin')
										input.form__field(type="text" name="from" placeholder="From")
									label
										+svg('pin')
										input.form__field(type="text" name="To" placeholder="To")
									label
										+svg('calendar_2')
										input.form__field(data-toggle="datepicker" type="text" name="date" placeholder="Date")

									select.form__select(name="transport")
										option(data-display="Choose transport") Choose transport
										option(value="value") Value 1
										option(value="value2") Value 2
										option(value="value3") Value 3
										option(value="value3") Value 4
